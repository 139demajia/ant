{
    "Lua.completion.autoRequire": false,
    "Lua.workspace.checkThirdParty": false,
    "diagnostics": {
        "disable": [
            "different-requires",
            "close-non-object",
            "need-check-nil"
        ],
        "globals": [
            "__EDITOR__",
            "import_feature",
            "import",
            "pipeline",
            "policy",
            "component",
            "feature",
            "system"
        ]
    },
    "runtime": {
        //"plugin": "misc/lsp-plugin.lua",
        "special": {
            "import_package": "require"
        }
    },
    "workspace": {
        "ignoreDir": [
            "pkg/ant.debugger",
            "pkg/ant.json",
            "pkg/ant.ecs/src",
            "tools/editor"
        ],
        "maxPreload": 100000
    }
}