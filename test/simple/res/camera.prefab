---
action:
    bind_camera: main_queue
data:
  scene:
    r: {0.5, 0, 0}  #3 parameter is euler angle
    t: {1, 2, -5, 1}
    updir: {0.0, 1.0, 0.0}
  camera:
    frustum:
      aspect: 1.3333333333333333
      f: 100
      fov: 60
      n: 1
      type: mat
  exposure:
    type          : "manual"
    aperture      : 16.0
    shutter_speed : 0.008
    ISO           : 100
tag:
  camera
policy:
  ant.camera|exposure
  ant.camera|camera
