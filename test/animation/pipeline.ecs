pipeline "init"
    .stage "init"
    .stage "init_blit_render"
    .stage "post_init"

pipeline "exit"
    .stage "exit"

pipeline "update"
    .stage "timer"
    .stage "data_changed"
    .pipeline "scene"
    .pipeline "collider"
    .pipeline "animation"
    .pipeline "sky"
    .stage "widget"
    .pipeline "render"
    .pipeline "select"
    .stage "update_editable_hierarchy"
    .pipeline "ui"
    .stage "end_frame"
    .stage "final"

pipeline "scene"
    .stage "update_hierarchy_scene"
    .stage "update_transform"

pipeline "collider"
    .stage "update_collider_transform"
    .stage "update_collider"

pipeline "animation"
    .stage "animation_state"
    .stage "sample_animation_pose"
    .pipeline "refine_animation_pose"
    .stage "skin_mesh"
    .stage "end_animation"

pipeline "refine_animation_pose"
    .stage "do_refine"
    .stage "do_ik"

pipeline "sky"
    .stage "update_sun"
    .stage "update_sky"

pipeline "render"
    .stage "shadow_camera"
    .stage "load_render_properties"
    .pipeline "handle_primitive"
    .stage "debug_shadow"
    .stage "cull"
    .stage "render_commit"
    .pipeline "postprocess"

pipeline "handle_primitive"
    .stage "filter_primitive"
    .stage "refine_filter"

pipeline "postprocess"
    .stage "bloom"
    .stage "tonemapping"
    .stage "combine_postprocess"

pipeline "select"
    .stage "update_select_view"
    .stage "update_filter_material"
    .stage "pickup"
    .stage "after_pickup"

pipeline "ui"
    .stage "ui_start"
    .stage "ui_update"
    .stage "ui_end"
