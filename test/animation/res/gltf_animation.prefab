---
{mount 1 root}
{mount 2 root}
---
policy:
    ant.render|render
    ant.render|mesh
    ant.animation|animation
    ant.animation|skinning
    ant.animation|animation_controller.birth
    ant.render|shadow_cast_policy
    ant.general|name
    ant.scene|hierarchy_policy
data:
    transform: $transform
        srt: $srt {}
    mesh: $resource "/pkg/ant.resources.binary/meshes/female/female.glb|mesh.meshbin:scenes.Root Scene.Beta_Joints.1"
    material: $resource /pkg/ant.resources/materials/gpu_skin.pbrm
    skeleton: $resource /pkg/ant.resources.binary/meshes/female/skeleton.ozz
    animation:
        anilist:
            idle: $resource /pkg/ant.resources.binary/meshes/female/animations/idle.ozz
            jumping: $resource /pkg/ant.resources.binary/meshes/female/animations/junmping.ozz
            punching: $resource /pkg/ant.resources.binary/meshes/female/animations/punching.ozz
            running: $resource /pkg/ant.resources.binary/meshes/female/animations/running.ozz
            running_fast: $resource /pkg/ant.resources.binary/meshes/female/animations/running-fast.ozz
            walking: $resource /pkg/ant.resources.binary/meshes/female/animations/walking.ozz
    animation_birth: idle
    can_render: true
    scene_entity: true
    can_cast: true
    name: gltf_animation_sample
    character:
        movespeed: 1.0
