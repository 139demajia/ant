---
{mount 1 root}
---
policy:
    ant.render|render
    ant.render|mesh
    ant.scene|hierarchy_policy
    ant.scene|transform_policy
    ant.general|name
data:
    transform: $transform
        srt: $srt
            s: {100, 100, 100, 0}
            t: {0, 0, 8, 1}
        slot: "mixamorig:Head"
    material: $resource /pkg/ant.resources/materials/bunny.material
    mesh: $resource "/pkg/ant.resources.binary/meshes/base/cube.glb|meshes/pCube1_P1.meshbin"
    can_render: true
    scene_entity: true
    name: slot_cube