---
policy:
    ant.render|render
    ant.animation|animation
    ant.animation|skinning
    ant.animation|animation_controller.birth
    ant.general|name
data:
    transform: $transform
        srt: $srt {}
    mesh: $resource "/pkg/ant.resources.binary/meshes/simple_skin/simple_skin.glb|meshes/mesh0_P1.meshbin"
    material: $resource /pkg/ant.resources/materials/gpu_skin.material
    skeleton: $resource /pkg/ant.resources.binary/meshes/simple_skin/animation/skeleton.ozz
    animation:
        anilist:
            idle: $resource /pkg/ant.resources.binary/meshes/simple_skin/animation/animation_0.ozz
    animation_birth: idle
    can_render: true
    scene_entity: true
    name: simple_skin_sample