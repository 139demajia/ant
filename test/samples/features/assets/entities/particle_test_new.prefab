---
policy:
    ant.effect|particle_emitter
    ant.general|name
data:
    name: emitter1
    material: /pkg/ant.resources/materials/particle/particle.material
    transform:{
        t: {2.0, 0.0, 0.0}
    }
    scene_entity: true
    state: 1
    emitter:
        lifetime:
            interp_type: linear
            minv: 30
            maxv: 40
        spawn:
            count:  6
            rate:   0.9
            init_lifetime:
                interp_type: linear
                minv: 5
                maxv: 6.5
            init_scale:
                interp_type: linear
                minv: {0.8, 0.8, 0.8}
                maxv: {0.85, 0.85, 0.85}
            scale_over_life:
                interp_type: linear
                minv: {1.0, 1.0, 1.0}
                maxv: {2.4, 2.4, 2.4}
            init_color:
                RGBA:
                    interp_type: const
                    value: {1.0, 1.0, 1.0, 1.0}
            color_over_life:
                RGB:
                    interp_type: linear
                    minv: {1.0, 1.0, 0.0}
                    maxv: {0.8, 0.0, 0.0}
                A:
                    interp_type: linear
                    minv: 1
                    maxv: 0.0
            init_velocity:
                interp_type: linear
                minv: {-0.1, 0.5, 0.0}
                maxv: {0.1, 0.5, 0.0}
            subuv_index:
                dimension: {8, 8}
                rate:
                    interp_type:const
                    value: 0.2