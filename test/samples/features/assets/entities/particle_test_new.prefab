---
policy:
    ant.effect|particle_system
    ant.general|name
data:
    name: particle_system_test
    particle_system: {}
---
policy:
    ant.effect|particle_emitter
    ant.general|name
data:
    name: emitter1
    material: /pkg/ant.resources/materials/particle/particle.material
    transform:{}
    scene_entity: true
    state: 1
    emitter:
        emitter_lifetime:
            interp_type: linear
            minv: 1.1
            maxv: 4
        spawn:
            count:  16
            rate:   1.0
        init_lifetime:
            interp_type: linear
            minv: 5
            maxv: 20
        init_scale:
            interp_type: linear
            minv:{1.15, 1.15, 1.15}
            maxv:{1.28, 1.28, 1.28}
        scale_over_life:
            interp_type: linear
            minv: {0.5, 1.0, 1.0}
            maxv: {3.0, 1.0, 1.0}
        init_color:
            RGBA:
                interp_type: linear
                minv: {0.0, 0.0, 0.0, 1.0}
                maxv: {1.0, 1.0, 1.0, 1.0}
        color_over_life:
            A:
                interp_type: linear
                minv: 1.0
                maxv: 0.0
            RGB:
                interp_type: const
                value: {1.0, 1.0, 1.0}
        init_velocity:
            interp_type: linear
            minv: {1.0, 0.0, 0.0}
            maxv: {0.0, 1.0, 1.0}
        uv_motion:
            scale:   1
            u_speed: 0.2
            v_speed: 0.3
action:
    attach_particle_system: 1