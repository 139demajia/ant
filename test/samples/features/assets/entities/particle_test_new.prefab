---
policy:
    ant.effect|particle_emitter
    ant.general|name
data:
    name: emitter1
    material: /pkg/ant.resources/materials/particle/particle.material
    transform:{
        t: {2.0, 0.0, 0.0}
    }
    scene_entity: true
    state: 1
    emitter:
        lifetime:
            interp_type: linear
            minv: 30
            maxv: 40
        spawn:
            count:  1
            rate:   1.0
            init_lifetime:
                interp_type: const
                value: 10
            init_scale:
                interp_type: const
                value: {1, 1, 1}
            init_color:
                RGBA:
                    interp_type: const
                    value: {1.0, 1.0, 1.0, 1.0}
            init_velocity:
                interp_type: const
                value: {0.0, 1.0, 0.0}
            subuv_index:
                dimension: {8, 8}
                rate: 
                    interp_type:const
                    value: 1