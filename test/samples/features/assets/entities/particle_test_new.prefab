---
policy:
    ant.effect|particle_emitter
    ant.general|name
data:
    name: emitter1
    material: /pkg/ant.resources/materials/particle/particle.material
    transform:{
        t: {2.0, 0.0, 0.0}
    }
    scene_entity: true
    state: 1
    emitter:
        lifetime:
            interp_type: linear
            minv: 30
            maxv: 40
        spawn:
            count:  10
            rate:   1.0
            init_lifetime:
                interp_type: linear
                minv: 1
                maxv: 2
            init_scale:
                interp_type: linear
                minv:{0.15, 0.15, 0.15}
                maxv:{1.28, 1.28, 1.28}
            init_translation:
                interp_type: const
                value:{0.0, 0.0, 0.0}
            scale_over_life:
                interp_type: linear
                minv: {0.5, 0.5, 0.5}
                maxv: {3.0, 3.0, 3.0}
            init_color:
                RGBA:
                    interp_type: linear
                    minv: {0.0, 0.0, 0.0, 1.0}
                    maxv: {1.0, 1.0, 1.0, 1.0}
            color_over_life:
                A:
                    interp_type: linear
                    minv: 1.0
                    maxv: 0.0
                RGB:
                    interp_type: const
                    value: {1.0, 1.0, 1.0}
            init_velocity:
                interp_type: linear
                minv: {1.0, 0.0, 0.0}
                maxv: {0.0, 1.0, 1.0}
