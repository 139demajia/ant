import "@ant.rmlui"
import "@ant.render"

system "init_system"
    .implement "init_system.lua"
    .require_system "ant.rmlui|rmlui_system"
    .require_interface "ant.render|irenderqueue"
    .method "post_init"
    .method "ui_update"

pipeline "init"
    .stage "init"
    .stage "post_init"

pipeline "exit"
    .stage "exit"

pipeline "update"
    .stage "timer"
    .stage "start_frame"
    .stage "data_changed"
    .pipeline "scene"
    .pipeline "camera"
    .pipeline "render"
    .stage "ui_start"
    .stage "ui_update"
    .stage "ui_end"
    .stage "end_frame"
    .stage "final"

