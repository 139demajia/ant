import "@ant.render"
import "@ant.asset"

system "load_properties_system"
    .implement "render_properties.lua"
    .require_interface "ant.render|uniforms"
    .require_interface "render_properties"
    .method "load_render_properties"

interface "render_properties"
    .implement "render_properties.lua"
    .method "data"

interface "uniforms"
    .implement "render_properties.lua"
    .method "system_uniform"
