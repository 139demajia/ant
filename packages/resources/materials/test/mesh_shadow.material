fx: $resource /pkg/ant.resources/materials/fx/test/mesh_shadow.fx
properties:
  s_basecolor: $property
    texture: $resource /pkg/ant.resources/textures/test/1x1_gray.texture
    stage: 0
  s_normal: $property
    texture: $resource /pkg/ant.resources/textures/test/1x1_normal.texture
    stage: 1
  u_fog_color: $property
    {0.95, 0.95, 0.95, 1}
  u_specularColor: $property
    {0.1, 0.1, 0.1, 1}
  u_specularLight: $property
    {1, 0, 0, 1}
state:
  ALPHA_REF: 0
  CULL: CCW
  DEPTH_TEST: LESS
  MSAA: true
  PT: TRISTRIP
  WRITE_MASK: RGBAZ