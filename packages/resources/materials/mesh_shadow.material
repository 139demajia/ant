fx:
  fs: /pkg/ant.resources/shaders/pbr/fs_pbr.sc
  vs: /pkg/ant.resources/shaders/pbr/vs_pbr.sc
  setting:
    shadow_receive: on
    PACK_TANGENT_TO_QUAT: 0
properties:
  u_pbr_factor: {1, 1, 0, 1}  #metallic, roughness, alpha_cutoff, occlusion strength
  u_emissive_factor: {0, 0, 0, 0}
  u_basecolor_factor: {0.8, 0.8, 0.8, 1.0}
state:
  ALPHA_REF: 0
  CULL: CCW
  DEPTH_TEST: GREATER
  MSAA: true
  PT: TRISTRIP
  WRITE_MASK: RGBAZ