system "skinning_system"
    .implement "skinning/skinning_system.lua"
    .require_system "animation_system"
    .method "skin_mesh"

transform "skinning_material"
    .implement "skinning/skinning_system.lua"
    .require_transform "ant.asset|material_setting_transform"
    .input "skinning_type"
    .input "_cache_prefab.material_setting"
    .output "_cache_prefab.material_setting.skinning"
    .method "process_prefab"