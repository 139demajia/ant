<rml><head>
<script type="text/x-lua">
	local model = window.createModel "basics" {
		title = "Particle Stat"
	}
    window.addEventListener("message", function(event)
    	--event.source.postMessage("Particle Stat")
		model.title = event.data
    end)
</script>
<style>
	panel {
		width: 200px;
		height: 24px;
		font: 150% "宋体";
		color: #FF0000;
	}
</style>
</head>
	<body onload="">
	<panel data-model="basics">
		{{title}}
	</panel>
</rml>
